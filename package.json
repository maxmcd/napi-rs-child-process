{
  "name": "napi-rs-child-process",
  "version": "0.0.6",
  "main": "index.js",
  "types": "index.d.ts",
  "napi": {
    "name": "napi-rs-child-process",
    "triples": {
      "defaults": false,
      "additional": [
        "aarch64-apple-darwin",
        "x86_64-apple-darwin",
        "x86_64-unknown-linux-gnu"
      ]
    }
  },
  "type": "module",
  "license": "MIT",
  "devDependencies": {
    "@napi-rs/cli": "^2.18.4",
    "vitest": "^2.0.4",
    "@types/node": "^20.12.7",
    "eslint": "^8.57.0",
    "@typescript-eslint/eslint-plugin": "^7.8.0",
    "eslint-plugin-import": "^2.29.1",
    "@types/eslint": "^8.44.4",
    "typescript": "^5.4.5"
  },
  "engines": {
    "node": ">= 10"
  },
  "repository": {
    "type": "git",
    "url": "git+https://github.com/maxmcd/napi-rs-child-process.git"
  },
  "scripts": {
    "artifacts": "napi artifacts",
    "build": "napi build --js=false --no-dts-header --release --platform",
    "build:ts": "npx tsc",
    "build:debug": "napi build --js=false --no-dts-header --platform",
    "prepublishOnly": "napi prepublish -t npm",
    "test": "vitest run",
    "bench": "vitest bench --run",
    "lint": "npx tsc --noEmit",
    "universal": "napi universal",
    "version": "napi version",
    "bump-version": "npx tsx scripts/bump-version.ts"
  }
}
